void leerArchivo2(){
	osada_file *object2=malloc(sizeof( osada_file));
	FILE * file= fopen("/home/utnso/tp-2016-2c-CompuMundoHiperMegaRed/cmhmr-osada/disco.bin", "rb");

	if (file != NULL) {
		fread(object2, sizeof(osada_file), 1, file);
		fclose(file);
	}
	printf("%i\n",object2->file_size);
}
